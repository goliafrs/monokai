{
  "tokenColors": [
    {
      "settings": {
        "background": "#282828",
        "foreground": "#F0F0F0",
        "caret": "#F0F0F0",
        "invisibles": "#3C3C3C",
        "lineHighlight": "#3C3C3C80",
        "findHighlight": "#FFE792",
        "findHighlightForeground": "#000000",
        "selection": "#646464",
        "selectionHighlight": "#64646466",
        "selectionBorder": "#222218",
        "activeGuide": "#9D550FB0",
        "bracketsForeground": "#F8F8F2A5",
        "bracketsOptions": "underline",
        "bracketContentsForeground": "#F8F8F2A5",
        "bracketContentsOptions": "underline",
        "tagsOptions": "stippled_underline"
      }
    },
    {
      "name": "Text",
      "scope": "text.xml",
      "settings": { "foreground": "#f0f0f0" }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "string.comment"
      ],
      "settings": { "foreground": "#787878" }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.value",
        "string.quoted.single.scss",
        "string.quoted.double.json",
        "string.quoted.double.html",
        "string.quoted.double.php",
        "meta.return-type",
        "meta.type.cast.expr",
        "meta.type.new.expr",
        "meta.preprocessor.string",
        "support.constant.math",
        "support.constant.dom",
        "support.constant.json",
        "markup.inline.raw",
        "invalid.illegal.bad-ampersand.html",
        "structure.dictionary.property-name.json",
        "constant.character.escape.scss"
      ],
      "settings": { "foreground": "#E6DB74" }
    },
    {
      "name": "REGEXP",
      "scope": [
        "constant.regexp",
        "string.regexp",
        "string.regexp.double-quoted.php"
      ],
      "settings": {
        "foreground": "#E6DB74"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric",
        "meta.preprocessor.numeric",
        "keyword.other.unit",
        "constant.sha.git-rebase"
      ],
      "settings": { "foreground": "#AE81FF" }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": { "foreground": "#AE81FF" }
    },
    {
      "name": "User-defined constant",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": { "foreground": "#AE81FF" }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "variable.css",
        "variable.scss",
        "variable.other.less",
        "variable.language",
        "support.variable",
        "support.other.variable",
        "support.type.vendored.property-name",
        "support.type.property-name",
        "meta.definition.variable.name",
        "meta.object-literal.key",
        "meta.object-literal.key entity.name.function",
        "storage.type.cs",
        "storage.type.generic.cs",
        "storage.type.modifier.cs",
        "storage.type.variable.cs",
        "storage.type.annotation.java",
        "storage.type.generic.java",
        "storage.type.java",
        "storage.type.object.array.java",
        "storage.type.primitive.array.java",
        "storage.type.primitive.java",
        "storage.type.token.java",
        "storage.type.groovy",
        "storage.type.annotation.groovy",
        "storage.type.parameters.groovy",
        "storage.type.generic.groovy",
        "storage.type.object.array.groovy",
        "storage.type.primitive.array.groovy",
        "storage.type.primitive.groovy"
      ],
      "settings": { "foreground": "#66D9EF" }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.operator.logical",
        "keyword.operator.constructor",
        "keyword.control",
        "keyword.other"
      ],
      "settings": { "foreground": "#F92672" }
    },
    {
      "name": "Keyword operator",
      "scope": [
        "keyword.operator",
        "keyword.operator.new",
        "keyword.operator.expression"
      ],
      "settings": { "foreground": "#F0F0F0" }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "storage.modifier"
      ],
      "settings": { "foreground": "#F92672" }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": { "foreground": "#F92672" }
    },
    {
      "name": "Class name",
      "scope": [
        "entity.name.class",
        "entity.name.module",
        "entity.name.type",
        "storage.identifier",
        "support.class"
      ],
      "settings": { "foreground": "#A6E22E" }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": { "foreground": "#A6E22E" }
    },
    {
      "name": "Function name",
      "scope": [
        "entity.name.function",
        "entity.name.function.js",
        "entity.name.type.js",
        "support.function"
      ],
      "settings": { "foreground": "#A6E22E" }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": { "foreground": "#FD971F" }
    },
    {
      "name": "Tag name",
      "scope": [
        "entity.name.tag",
        "entity.name.tag.class.js",
        "entity.name.tag.class.jsx"
      ],
      "settings": { "foreground": "#F92672" }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": { "foreground": "#A6E22E" }
    },
    {
      "name": "Library constant",
      "scope": [
        "support.constant",
        "support.constant.dom"
      ],
      "settings": { "foreground": "#66D9EF" }
    },
    {
      "name": "Language Variable",
      "scope": "variable.language",
      "settings": { "foreground": "#F92672" }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "background": "#F92672",
        "foreground": "#F0F0F0"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "background": "#AE81FF",
        "foreground": "#F0F0F0"
      }
    },
    {
      "name": "Diff",
      "scope": [
        "meta.diff",
        "meta.diff.header"
      ],
      "settings": { "foreground": "#787878" }
    },
    {
      "name": "Markup: deleted",
      "scope": "markup.deleted",
      "settings": { "foreground": "#F92672" }
    },
    {
      "name": "Markup: inserted",
      "scope": "markup.inserted",
      "settings": { "foreground": "#A6E22E" }
    },
    {
      "name": "Markup: changed",
      "scope": "markup.changed",
      "settings": { "foreground": "#E6DB74" }
    },
    {
      "name": "Markup: emphasis",
      "scope": [
        "markup.italic",
        "markup.italic.markdown"
      ],
      "settings": { "fontStyle": "italic" }
    },
    {
      "name": "Markup: emphasis punctuation",
      "scope": "punctuation.definition.italic.markdown",
      "settings": {
        "fontStyle": "none",
        "foreground": "#FF0000"
      }
    },
    {
      "name": "Markup: underline",
      "scope": "markup.underline",
      "settings": { "fontStyle": "underline" }
    },
    {
      "name": "Markup: bold",
      "scope": "markup.bold",
      "settings": { "fontStyle": "bold" }
    },
    {
      "name": "Markup: selector",
      "scope": "markup.selector",
      "settings": { "foreground": "F0F0F0" }
    },
    {
      "name": "Markdown: link",
      "scope": "markup.underline.link.markdown",
      "settings": { "foreground": "#AE81FF" }
    },
    {
      "name": "Markdown: bold",
      "scope": "markup.bold.markdown",
      "settings": { "fontStyle": "bold" }
    },
    {
      "name": "Markdown: bold punctuation",
      "scope": "punctuation.definition.bold.markdown",
      "settings": {
        "fontStyle": "none",
        "foreground": "#FF0000"
      }
    },
    {
      "name": "Markdown: heading",
